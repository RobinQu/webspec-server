<div class="row">
  <div class="col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading"><%=  __("Test suites %s", repo) %></div>
      <div class="panel-body">
        <dl>
          <dt><%= __("Runner") %></dt>
          <dd><%= webspec.runner %></dd>
          <dt><%= __("Repository") %></dt>
          <dd><%= owner %>/<%= repo %></dd>
          <dt><%= __("Sources") %></dt>
          <dd><pre><%= JSON.stringify(webspec.sources) %></pre></dd>
          <dt><%= __("Specs") %></dt>
          <dd><pre><%= JSON.stringify(webspec.specs) %></pre></dd>
          <dt><%= __("Revision") %></dt>
          <dd><%= ref %></dd>
        </dl>
        <a href="/inspect/?owner=<%= owner %>&repo=<%= repo %>" class="btn btn-primary btn-block"><%= __("View detail of %s", repo) %></a>
      </div>
    </div>
    
    <%- include _sidebar %>
    
  </div>
  <div class="col-md-8">
    <%- include _sandbox %>
  </div>
</div>